{
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es6",
    "jsx": "react",
    "lib": [
      "dom",
      "esnext",
      "es5",
      "es6",
      "dom.iterable"
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "node",
    ],
    "baseUrl": "./src",
    "paths": {
      "@middleware/*": [
        "middleware/*"
      ],
      "@controller/*": [
        "controller/*"
      ],
      "@routes/*": [
        "routes/*"
      ],
      "@configs/*": [
        "configs/*"
      ],
      "@seeder/*": [
        "seeder/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@models/*": [
        "models/*"
      ]
    }
  },
  "include": [
    "src/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".vscode",
  ],
  "files": [
    "./server.ts",
  ],
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}